		<autoload name="JobValidator">%core.lib_dir%/validator/JobValidator.class.php</autoload>
		<autoload name="CategoryValidator">%core.lib_dir%/validator/CategoryValidator.class.php</autoload>
		<autoload name="CategoryNameValidator">%core.lib_dir%/validator/CategoryNameValidator.class.php</autoload>
		<autoload name="LanguageValidator">%core.lib_dir%/validator/LanguageValidator.class.php</autoload>
	</ae:configuration>
</ae:configurations>