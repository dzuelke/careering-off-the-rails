		<autoload name="Doctrine\Common\ClassLoader">%core.vendor_dir%/doctrine2-orm/lib/vendor/doctrine-common/lib/Doctrine/Common/ClassLoader.php</autoload>

		<autoload name="Doctrine2Database">%core.lib_dir%/database/Doctrine2Database.class.php</autoload>
		<autoload name="Doctrine2AgaviModel">%core.lib_dir%/database/Doctrine2AgaviModel.class.php</autoload>
		<autoload name="Doctrine2Configuration">%core.lib_dir%/database/Doctrine2Configuration.class.php</autoload>
		<autoload name="Doctrine2ContextEventListener">%core.lib_dir%/database/Doctrine2ContextEventListener.class.php</autoload>

		<autoload name="Zend_Search_Lucene">%core.vendor_dir%/Zend/Search/Lucene.php</autoload>

		<autoload name="JobeetUser">%core.lib_dir%/user/JobeetUser.class.php</autoload>
